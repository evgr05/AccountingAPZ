<Window x:Class="AccountingAPZ.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AccountingAPZ"
        mc:Ignorable="d"
        Title="Учет изготавливаемой продукции" 
        Height="600" Width="900"
        MinHeight="450" MinWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="55"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Orientation="Horizontal">
            <Image Source="/img/APZ.png"/>
            <Label x:Name="headLbl" Content="Производство продукции" FontSize="35"/>
        </StackPanel>
        <Frame Name="frmMain" NavigationUIVisibility="Hidden" Grid.Row="1"/>
    </Grid>
</Window>
